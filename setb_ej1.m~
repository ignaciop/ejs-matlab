clear all

syms t;
s = dsolve('t*Dy + 3*y == 5*t^2', 'y(2) == 5','t')

figure(1)
ezplot(s,[0.5 5])
axis tight
grid on

figure(2)
ezplot(s,[0.2 20])
axis tight
grid on



ss = dsolve('t*Dy + 3*y == 5*t^2', 'y(2) == 3','t')

figure(3)
ezplot(ss,[0.5 5])
axis tight
grid on

figure(4)
ezplot(ss,[0.2 20])
axis tight
grid on



sss = dsolve('t*Dy + 3*y == 5*t^2', 'y(2) == c','t')

figure(5)
hold on
for cval = 3:7
    ezplot(subs(sss
